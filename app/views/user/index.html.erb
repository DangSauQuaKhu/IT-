<!--- start --->
<center>
  <div class="container" id="Blogs">
  
    <% @users.each do |user| %>
        <div class="card-deck">
          <div class="card" style="max-width: 350px">
            <div class="card-body">
              <h5 class="card-title"><%= user.email %> </h5>
        
            </div>
            <div class="card-footer">
            
              <small class="text-muted">
                created at <%= user.updated_at.strftime("%d-%m-%Y") %>
              </small>
            </div>
          </div>
        </div>
          <% end %>
    </div>
          
</center>
